{
    "version": 2,
    "builds": [
      {
        "src": "server.mjs",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/server.mjs"
      }
    ],
    "functions": {
      "api/**/*.js": {
        "maxDuration": 5
      }
    }
  }
  